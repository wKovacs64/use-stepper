{
  "name": "use-stepper",
  "version": "0.0.0-semantically-released",
  "description": "React hook to manage a numeric stepper",
  "keywords": [
    "react",
    "hook",
    "hooks",
    "stepper",
    "counter",
    "increment",
    "decrement",
    "input"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "source": "src/use-stepper.ts",
  "main": "dist/use-stepper.js",
  "module": "dist/use-stepper.m.js",
  "unpkg": "dist/use-stepper.umd.js",
  "types": "dist/use-stepper.d.ts",
  "files": [
    "dist",
    "!dist/**/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/use-stepper.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/use-stepper/issues"
  },
  "homepage": "https://github.com/wKovacs64/use-stepper#readme",
  "scripts": {
    "build": "microbundle",
    "clean": "rimraf dist coverage",
    "check-types": "tsc",
    "cm": "git-cz",
    "develop": "microbundle watch",
    "format": "prettier --write .",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "postinstall": "patch-package",
    "prebuild": "npm run --silent clean",
    "prepare": "npm-run-all --silent husky:install build",
    "prepublishOnly": "npm-run-all --silent --sequential lint check-types test:coverage",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "validate": "npm-run-all --silent --sequential lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.1.0",
    "@types/jsdom": "16.2.14",
    "@types/node": "16.11.27",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/ws": "8.5.3",
    "@wkovacs64/prettier-config": "3.0.1",
    "c8": "7.11.0",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "doctoc": "2.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-wkovacs64": "13.13.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "microbundle": "0.14.2",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.70.2",
    "semantic-release": "19.0.2",
    "typescript": "4.6.3",
    "vitest": "0.9.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  }
